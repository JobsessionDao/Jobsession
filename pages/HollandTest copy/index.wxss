.cyBgLeft {
    position: fixed;
    top: 0;
    left: 0;
    filter: blur(80rpx);
    width: 600rpx;
    height: 200rpx;
    background-color: rgba(78, 115, 255, 0.36);
}

.cyBgRight {
    position: fixed;
    top: 100rpx;
    right: 0;
    filter: blur(80rpx);
    width: 600rpx;
    height: 200rpx;
    background-color: rgba(255, 87, 51, 0.16);
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.cyQuestion {
    display: block;
    width: 90%;
    font-size: 24px;
    font-weight: 700;
}

.cyOptionBlock{
    position: fixed;
    width: 100%;
    bottom: 100rpx;
    display: flex;
    justify-content: center;
    flex-direction: column;
    align-items: center;
} 

.cyHoBotton_color_b{
    margin-top: 15rpx;
    width: 65%;
    height: 100rpx;
    line-height: 100rpx;
    border-radius: 25rpx;
    font-size: 18px;
    font-weight: 700;
    color: rgba(61, 97, 255, 1);
    text-align: center;
    background-color: rgba(86, 125, 244, 0.1);
}

.cyHoBotton_hover_color_b{
    width: 62%;
    height: 95rpx;
    line-height: 95rpx;
    background-color: rgba(86, 125, 244, 0.1);
    transition: all 300ms ease .5ms;
}

.cyHoBotton_color_r{
    margin-top: 15rpx;
    width: 65%;
    height: 100rpx;
    line-height: 100rpx;
    border-radius: 25rpx;
    font-size: 18px;
    font-weight: 700;
    color: rgba(255, 87, 51, 1);
    text-align: center;
    background-color: rgba(255, 87, 51, 0.13);
}


.cyHoBotton_hover_color_r{
    width: 62%;
    height: 95rpx;
    line-height: 95rpx;
    background-color: rgba(255, 87, 51, 0.13);
    transition: all 300ms ease .5ms;
}

